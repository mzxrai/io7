{
  "name": "io7",
  "compatibility_date": "2025-01-29",
  "build": {
    "command": "cd frontend && npm ci && npm run build"
  },
  "routes": [
    {
      "pattern": "io7.dev",
      "custom_domain": true
    },
    {
      "pattern": "www.io7.dev",
      "custom_domain": true
    }
  ],
  "assets": {
    "directory": "./frontend/dist",
    "html_handling": "auto-trailing-slash",
    "not_found_handling": "single-page-application"
  }
}