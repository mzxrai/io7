{
  "name": "io7",
  "version": "0.1.1",
  "description": "Subagent registry for Claude Code - Install specialized AI agents to enhance your Claude Code workflows",
  "type": "module",
  "bin": {
    "io7": "cli/index.built.js"
  },
  "files": [
    "cli/index.built.js",
    "README.md",
    "LICENSE.txt"
  ],
  "keywords": [
    "claude",
    "claude-code",
    "ai",
    "subagents",
    "cli",
    "developer-tools",
    "automation",
    "ai-agents"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mzxrai/io7.git"
  },
  "bugs": {
    "url": "https://github.com/mzxrai/io7/issues"
  },
  "homepage": "https://io7.dev",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && npm run build:cli",
    "build:cli": "node scripts/build-cli.js",
    "build:cli:dev": "NODE_ENV=development node scripts/build-cli.js",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "prepublishOnly": "npm run build:cli"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@testing-library/dom": "^10.4.1",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.32.0",
    "eslint-plugin-import": "^2.32.0",
    "happy-dom": "^18.0.1",
    "typescript": "~5.8.3",
    "vite": "^7.0.4",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "lucide": "^0.536.0"
  }
}
